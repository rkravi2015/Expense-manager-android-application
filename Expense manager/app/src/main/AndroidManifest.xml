<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.rcplproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AddTripActivity" />
        <activity android:name=".ProjectActivity" />
        <activity android:name=".MainMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewTripListActivity" />
        <activity android:name=".ViewTripActivity" />
        <activity android:name=".ExpenseActivity" />
        <activity android:name=".ViewReceiptImageActivity" />
        <activity android:name=".LowBudgetActivity"></activity>
    </application>

</manifest>